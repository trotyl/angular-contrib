{
  "name": "@angular-contrib/workspace",
  "version": "0.0.2",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "yarn test:core && yarn test:common",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "codecov": "codecov",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1",
    "build:core": "ng build core",
    "postbuild:core": "cp README.md dist/core",
    "build:common": "ng build common",
    "postbuild:common": "cp README.md dist/common",
    "build:all": "yarn build:core && yarn build:common",
    "test:core": "ng test core --no-watch --code-coverage --no-progress",
    "test:common": "ng test common --no-watch --code-coverage --no-progress",
    "publish:core": "cd dist/core && npm publish",
    "publish:common": "cd publish/common && npm publish",
    "publish:all": "yarn publish:core && yarn publish:common"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.1",
    "@angular/common": "^7.0.1",
    "@angular/compiler": "^7.0.1",
    "@angular/core": "^7.0.1",
    "@angular/forms": "^7.0.1",
    "@angular/http": "^7.0.1",
    "@angular/platform-browser": "^7.0.1",
    "@angular/platform-browser-dynamic": "^7.0.1",
    "@angular/router": "^7.0.1",
    "core-js": "^2.5.7",
    "rxjs": "^6.3.3",
    "tslib": "^1.9.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.10.3",
    "@angular-devkit/build-ng-packagr": "^0.10.3",
    "@angular/cli": "^7.0.3",
    "@angular/compiler-cli": "^7.0.1",
    "@angular/language-service": "^7.0.1",
    "@types/jasmine": "^2.8.8",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^8.9.4",
    "codecov": "^3.1.0",
    "codelyzer": "^4.5.0",
    "conventional-changelog-cli": "^2.0.5",
    "jasmine-core": "2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.1",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.4.0",
    "protractor": "^5.4.0",
    "ts-node": "~4.1.0",
    "tsickle": "0.32.1",
    "tslint": "^5.11.0",
    "typescript": "~3.1.4"
  }
}
