{
  "name": "@angular-contrib/workspace",
  "version": "0.0.7",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "yarn test:core && yarn test:common",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "codecov": "codecov",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1",
    "build:core": "ng build core",
    "postbuild:core": "cp README.md dist/core",
    "build:common": "ng build common",
    "postbuild:common": "cp README.md dist/common",
    "build:all": "yarn build:core && yarn build:common",
    "postbuild:all": "yarn fill-package-json",
    "test:core": "ng test core --no-watch --code-coverage --no-progress",
    "test:common": "ng test common --no-watch --code-coverage --no-progress",
    "publish:core": "cd dist/core && yarn publish",
    "publish:common": "cd dist/common && yarn publish",
    "publish:all": "yarn publish:core && yarn publish:common",
    "fill-package-json": "ts-node --project tsconfig.node.json tools/fill-package-json.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.7",
    "@angular/common": "^8.2.7",
    "@angular/compiler": "^8.2.7",
    "@angular/core": "^8.2.7",
    "@angular/forms": "^8.2.7",
    "@angular/platform-browser": "^8.2.7",
    "@angular/platform-browser-dynamic": "^8.2.7",
    "@angular/router": "^8.2.7",
    "core-js": "^3.3.6",
    "rxjs": "^6.5.3",
    "tslib": "^1.9.3",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.17",
    "@angular-devkit/build-ng-packagr": "^0.803.17",
    "@angular/cli": "^8.3.17",
    "@angular/compiler-cli": "^8.2.7",
    "@angular/language-service": "^11.2.0",
    "@types/jasmine": "^3.4.5",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.12.5",
    "codecov": "^3.6.1",
    "codelyzer": "^5.2.0",
    "conventional-changelog-cli": "^2.0.25",
    "handlebars": "^4.5.1",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.0.1",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^5.7.1",
    "protractor": "^5.4.0",
    "ts-node": "^8.4.1",
    "tsickle": "^0.37.0",
    "tslint": "^5.20.0",
    "typescript": "~3.5.3"
  }
}
